{
  "name": "xShip",
  "plugin": "plugin.video.xship",
  "priority": 100,
  "provider": "xShip",
  "is_resolvable": "true",
  "assert": {
    "play_movie": [
      "title"
    ],
    "play_episode": [
      "title",
      "season_number",
      "episode_number"
    ],
    "search_movie": [
      "title"
    ],
    "search_series": [
      "title"
    ]
  },
  "play_movie": [
    "plugin://plugin.video.xship/?action=moviesSearch",
    {
      "movie_id": "{movie_id}",
      "autoplay": "true"
    },
    {
      "label": "(?i).*{title}.*"
    }
  ],
  "play_episode": [
    "plugin://plugin.video.xship/?action=tvshowsSearch",
    {
      "series_id": "{series_id}",
      "season_number": "{season_number}",
      "episode_number": "{episode_number}",
      "autoplay": "true"
    },
    {
      "label": "(?i).*{title}.*"
    }
  ],
  "search_movie": [
    "plugin://plugin.video.xship/?action=tvshowsSearch",
    {
      "search": "{title}"
    }
  ],
  "search_series": [
    "plugin://plugin.video.xship/?action=searhtvshowsSearch",
    {
      "search": "{title}"
    }
  ]
}
