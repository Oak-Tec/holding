{
  "name": "xShip",
  "plugin": "plugin.video.xship",
  "priority": 100,
  "provider": "xShip",
  "is_resolvable": true,
  "assert": {
    "play_movie": [
      "title"
    ],
    "play_episode": [
      "showname",
      "season",
      "episode"
    ]
  },
  "play_movie": [
    "plugin://plugin.xship/?action=searchNew&table=movie&query=$INFO[Skin.String(SearchInputEncoded)]"
    {
      "keyboard": "{title}"
    },
    {
      "label": "(?i).*{title}.*"
    }
  ],
  "play_episode": [
    "plugin://plugin.xship/?action=searchNew&table=tvshows&query=$INFO[Skin.String(SearchInputEncoded)]"
    {
      "return": true,
      "season": "{season}",
      "episode": "{episode}"
    },
    {
      "season": "{season}"
    },
    {
      "episode": "{episode}"
    }
  ]
}
