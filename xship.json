{
  "name": "xShip",
  "plugin": "plugin.video.xship",
  "priority": 100,
  "provider": "xShip",
  "is_resolvable": true,
  "assert": {
    "play_movie": [
      "title"
    ],
    "play_episode": [
      "showname",
      "season",
      "episode"
    ]
  },
  "play_movie": [
    "plugin://plugin.video.xship/?action=New&table=movies&title={title}"
  ],
  "play_episode": [
    "plugin://plugin.video.xship/?action=New&table=tvshows&showname={showname}&season={season}&episode={episode}"
  ]
}
