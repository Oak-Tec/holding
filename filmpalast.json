{
  "name": "Filmpalast",
  "plugin": "plugin.video.xstream",
  "priority": 100,
  "provider": "Filmpalast",
  "is_resolvable": "true",
  "assert": {
    "play_movie": [
      "title"
    ],
    "search_movie": [
      "title"
    ]
  },
  "play_movie": [
    "plugin://plugin.video.xstream/?site=filmpalast&function=showSearch",
    {
      "search": "{title}"
    },
    {
      "label": "(?i).*{title}.*"
    }
  ],
  "search_movie": [
    "plugin://plugin.video.xstream/?site=filmpalast&function=showSearch",
    {
      "search": "{title}"
    }
  ]
}
