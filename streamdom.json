{
    "name": "Streamdom",
    "plugin": "plugin.video.xstream",
    "priority": 100,
    "provider_name": "Streamdom",
    "is_resolvable": true,
    "assert": {
        "play_movie": [
            "title"
        ],
        "play_episode": [
            "showname",
            "season",
            "episode"
        ]
    },
    "play_movie": "plugin://plugin.video.xstream/?site=filmpalast&function=showSearch&query={title}",
    "play_episode": "plugin://plugin.video.xstream/?site=serienstream&function=showSearch&query={showname}&season={season}&episode={episode}"
}
