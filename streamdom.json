{
  "name": "Streamdom",
  "plugin": "plugin.video.xstream",
  "priority": 100,
  "provider": "Streamdom",
  "is_resolvable": "true",
  "assert": {
    "play_movie": [
      "title"
    ],
    "play_episode": ["showname", "season", "episode"]
  },
  "play_movie": [
    "plugin://plugin.video.xstream/?site=filmpalast&function=showSearch",
    {
      "search": "{title}"
    },
    {
      "label": "(?i).*{title}.*"
    }
  ],
  "play_episode": [
    "plugin://plugin.video.xstream/?site=serienstream&function=showSearch",
    { "keyboard": "{showname}" },
    { "label": "(?i).*{showname}.*" },
    { "return": "true", "season": "{season}", "episode": "{episode}" },
    { "season": "{season}" },
    { "season": "{season}", "episode": "{episode}" }
  ]
}
